            <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="Assert whole response matches db" enabled="true">
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="script">import groovy.json.JsonSlurper

def user = new JsonSlurper().parse(prev.getResponseData(), &apos;UTF-8&apos;)
def dbUser = vars.getObject(&quot;dbUser&quot;).get(0)
def dbUserProfiles = vars.getObject(&quot;dbProfiles&quot;)
def typeEnumMap = [0:&quot;GITHUB&quot;, 1:&quot;TWITTER&quot;, 2: &quot;LINKEDIN&quot;]

assert dbUser.get(&quot;ID&quot;) == user.id
assert dbUser.get(&quot;NAME&quot;) == user.name
assert dbUser.get(&quot;EMAIL&quot;) == user.email

if (dbUserProfiles.size() == 0) {
	assert user.profiles == null
} else {
	assert dbUserProfiles.size() == user.profiles.size()
	for(def dbUserProfile : dbUserProfiles) {
		boolean profileFound = false;
		for (def userProfile : user.profiles) {
			if (dbUserProfile.get(&quot;ID&quot;) == userProfile.id) {
				profileFound = true;
				
				assert dbUserProfile.get(&quot;EXTERNAL_ID&quot;) == userProfile.externalId
				assert typeEnumMap.get(dbUserProfile.get(&quot;TYPE&quot;)) == userProfile.type

				log.info(&quot;Validated profile id &quot; + dbUserProfile.get(&quot;ID&quot;) + &quot; for user id &quot; + user.id)
			}
		}
		assert profileFound : &quot;Profile id &quot; + dbUserProfile.get(&quot;ID&quot;) + &quot; for user id &quot; + user.id + &quot; not found&quot;
	}
}</stringProp>
